  lightGray = #eee
  red = #be2525

  vendor(property, arg)
    -webkit-{property} arg
    {property} arg

  border-radius(radius = 10px)
    vendor('border-radius', radius)

  transition(args)
    vendor('transition', args)

  clear-fix()
    zoom 1
    &:after
    &:before
      content ""
      display table
    &:after
      clear both

  add(a, b, c = a)
    a + b + c

  marginer(a, b)
    return a a*2 a*b b